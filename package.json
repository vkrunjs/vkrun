{
  "name": "vkrun",
  "version": "3.0.5",
  "description": "Vkrun is a Node.js framework for building server-side applications",
  "author": "Mario Elvio",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "vkrun",
    "vkrunjs",
    "framework",
    "web",
    "http",
    "rest",
    "restful",
    "router",
    "app",
    "api",
    "schema",
    "cors",
    "validate",
    "validator",
    "rate limit",
    "parse data",
    "jwt",
    "logger",
    "super request",
    "mime",
    "serve static file",
    "swagger builder",
    "upload",
    "backend",
    "back-end"
  ],
  "files": [
    "LICENSE",
    "Readme.md",
    "lib",
    "Publishing-Guide.md",
    "CHANGELOG.md"
  ],
  "homepage": "https://github.com/vkrunjs/vkrunjs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vkrunjs/vkrunjs.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest --coverage=false",
    "test:cors": "jest --selectProjects cors",
    "test:jwt": "jest --selectProjects jwt",
    "test:load-env": "jest --selectProjects load-env",
    "test:location": "jest --selectProjects location",
    "test:logger": "jest --selectProjects logger",
    "test:mime": "jest --selectProjects mime",
    "test:parse-data": "jest --selectProjects parse-data",
    "test:rate-limit": "jest --selectProjects rate-limit",
    "test:router": "jest --selectProjects router",
    "test:schema": "jest --selectProjects schema",
    "test:server-static-file": "jest --selectProjects server-static-file",
    "test:super-request": "jest --selectProjects super-request",
    "test:swagger-ui": "jest --selectProjects swagger-ui",
    "test:upload": "jest --selectProjects upload",
    "test:utils": "jest --selectProjects utils",
    "test:validate-route-data": "jest --selectProjects validate-route-data",
    "test:coverage": "jest",
    "test:staged": "jest --passWithNoTests",
    "test:ci": "npm test -- --coverage=false",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --ext *.ts --fix",
      "npm run test:staged",
      "git add"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/js": "^9.21.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "@types/pidusage": "^2.0.5",
    "eslint": "^9.23.0",
    "eslint-config-love": "^118.0.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.4",
    "form-data": "^4.0.2",
    "globals": "^16.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "*",
    "typescript-eslint": "^8.24.1"
  }
}
